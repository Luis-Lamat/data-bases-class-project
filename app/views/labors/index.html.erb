<h1>Listing labors</h1>

<table>
  <thead>
    <tr>
      <th>Estimated time</th>
      <th>Description</th>
      <th>Cost</th>
      <th>External comision</th>
      <th>Internal comision</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @labors.each do |labor| %>
      <tr>
        <td><%= labor.estimated_time %></td>
        <td><%= labor.description %></td>
        <td><%= labor.cost %></td>
        <td><%= labor.external_comision %></td>
        <td><%= labor.internal_comision %></td>
        <td><%= link_to 'Show', labor %></td>
        <td><%= link_to 'Edit', edit_labor_path(labor) %></td>
        <td><%= link_to 'Destroy', labor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Labor', new_labor_path %>
